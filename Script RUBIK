CREATE DATABASE RUBIK_PRODUCTIONSXX;

USE RUBIK_PRODUCTIONSXX;

CREATE TABLE  RUBIK_PRODUCTIONSXX.PELICULAS(
	        
                MOVIE_TITLE	VARCHAR(20)	NOT NULL
                , FIRST_COUNTRY	VARCHAR(20)	NOT NULL
                , CATEGORY	VARCHAR(20)	NOT NULL
                , BRAND 	VARCHAR(20)	NOT NULL
                , TOTAL_INVESTMENT	DECIMAL	NOT NULL
		, ID_PELICULA	INT 	NOT NULL 
		, REALEASE_DATE	  DATE	NOT NULL    
		, PRIMARY KEY (ID_PELICULA)
);
  
  
  hacer bien en cines unico el id de theater, no suplicado de id_cines xq es primary key. 
  borrar gross y admissions y ponerlo por peliculas, porque ahi si que se puede repetir 

CREATE TABLE  RUBIK_PRODUCTIONSXX.CINES(
	                   ID_MOVIE_TEATHER     INT    NOT NULL
                         , GROSS_OPENDWEEKEND_CINES	NUMERIC  NOT NULL
                         , ADMISSIONS_TOTALES_CINES	INT NOT NULL
                         , COST_TICKET	        BIGINT NOT NULL
                         , Q_TICKETS_TOTALES	INT   NOT NULL
			 , NAME_MOVIE_TEATHER   VARCHAR(20) NOT NULL
	                 , PRIMARY KEY (ID_MOVIE_TEATHER)
);


CREATE TABLE  RUBIK_PRODUCTIONSXX.STREAMING (
	            ID_STREAMING	INT 	NOT NULL
                 , STREAMING_NAME	VARCHAR(20)	NOT NULL
                 , SUBSCRIPTION_COST	BIGINT	NOT NULL
		 ,GROSS_OPENDWEEKEND_STREAMING    INT 	NOT NULL
		  ,ADMISSIONS_TOTALES_STREAMING    INT 	NOT NULL
                 , PRIMARY KEY (ID_STREAMING)
                     );
		     
  CREATE TABLE  RUBIK_PRODUCTIONSXX.PUBLICO_MOVIE_THEATER (
                                                           ID_MOVIE_THEATER           INT 	        NOT NULL
							 , USER_ID_MOVIE_THEATER      INT 	        NOT NULL
							 , NOMBRE                     VARCHAR(20)	NOT NULL
							 , APELLIDO                   VARCHAR(20)	NOT NULL
							 , NACIONALIDAD               VARCHAR(20)	NOT NULL
							 , EDAD                      VARCHAR(20)	NOT NULL
							 , GENERO                    VARCHAR(20)	NOT NULL
							 , DNI                         INT 	        NOT NULL
							 , ID_PELICULA_STR             INT 	        NOT NULL
							 ,PRIMARY KEY ( USER_ID_MOVIE_THEATER)
							 );
		   
		     
CREATE TABLE  RUBIK_PRODUCTIONSXX. PUBLICO_STREAMING 
                              (USER_ID_STREAMING      INT 	        NOT NULL
			      , ID_STREAMING_STR      INT 	        NOT NULL
			      , NOMBRE                VARCHAR(20)	NOT NULL
			      , APELLIDO              VARCHAR(20)	NOT NULL
			      , NACIONALIDAD          VARCHAR(20)	NOT NULL
			      , EDAD                 INT 	        NOT NULL
			      , GENERO                VARCHAR(20)	NOT NULL
			      , DNI                  INT 	        NOT NULL
			      , ID_PELICULA_ST       INT 	        NOT NULL
			      ,PRIMARY KEY (USER_ID_STREAMING)
			      );




CREATE TABLE  RUBIK_PRODUCTIONSXX.CAMPAÑA_MARKETING (
                                                     OBJETIVO                VARCHAR(20)	NOT NULL
						    ,ID_PELICULA_            INT 	        NOT NULL
                                                    ,PRESUPUESTO             INT 	        NOT NULL
						    ,MEDIO_PRINCIPAL         VARCHAR(20)	NOT NULL 
						    ,RESPONSABLE_CAMPAÑA     VARCHAR(20)	NOT NULL  
						    ,ID_CAMPAÑA_MARKETING    INT 	        NOT NULL
						    , PRIMARY KEY (ID_CAMPAÑA_MARKETING)
						    );






